<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.iit.secretpuppy">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".activity.LeaderBoardActivity"></activity>
        <activity android:name=".activity.LaunchScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.SearchBreedActivity" />
        <activity android:name=".activity.IdentifyDogActivity" />
        <activity android:name=".activity.IdentifyBreedActivity" />
        <activity android:name=".activity.MainActivity" />
    </application>

    <uses-permission android:name="android.permission.VIBRATE"/>

</manifest>